{
   "name": "kitto",
   "version": "0.1.0",
   "license": "MIT",
   "description": "A unique blend of items for any web expedition",
   "author": "Matt Pilott",
   "homepage": "https://github.com/mattpilott/kitto#readme",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/mattpilott/kitto.git"
   },
   "bugs": {
      "url": "https://github.com/mattpilott/kitto/issues"
   },
   "publishConfig": {
      "access": "public"
   },
   "main": "lib/index.js",
