{
	"name": "kitto",
	"version": "1.0.0-rc.9",
	"author": "Matt Pilott",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mattpilott/kitto.git"
	},
	"devDependencies": {
		"@eslint/compat": "1.2.9",
		"@eslint/js": "9.28.0",
		"@microsoft/api-documenter": "^7.26.27",
		"@microsoft/api-extractor": "^7.52.8",
		"@playwright/test": "1.52.0",
		"@sveltejs/adapter-auto": "6.0.1",
		"@sveltejs/kit": "2.21.2",
		"@sveltejs/package": "2.3.11",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/svelte": "5.2.8",
		"@types/node": "22.15.30",
		"@vitest/coverage-v8": "3.2.2",
		"eslint": "9.28.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-svelte": "3.9.1",
		"globals": "16.2.0",
		"jsdom": "26.1.0",
		"lightningcss": "1.30.1",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"publint": "0.3.12",
		"svelte": "5.33.14",
		"svelte-check": "4.2.1",
		"tinydate": "1.3.0",
		"typedoc": "0.28",
		"typedoc-github-theme": "0.3.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.33.1",
		"vite": "6.3.5",
		"vitest": "3.2.2"
	},
	"exports": {
		".": {
			"types": "./dist/vanilla/index.d.ts",
			"svelte": "./dist/vanilla/index.js"
		},
		"./vanilla": {
			"types": "./dist/vanilla/index.d.ts",
			"svelte": "./dist/vanilla/index.js"
		},
		"./svelte": {
			"types": "./dist/svelte/index.d.ts",
			"svelte": "./dist/svelte/index.js"
		},
		"./lightningcss": {
			"types": "./dist/lightningcss/index.d.ts",
			"svelte": "./dist/lightningcss/index.js"
		},
		"./reset": "./dist/styles/reset.css",
		"./unify": "./dist/styles/unify.css"
	},
	"bugs": {
		"url": "https://github.com/mattpilott/kitto/issues"
	},
	"description": "A unique blend of tree-shakeable items for any web expedition",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"homepage": "https://github.com/mattpilott/kitto#readme",
	"keywords": [
		"svelte",
		"utilities"
	],
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"bench": "vitest bench",
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"docs": "typedoc",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"coverage": "vitest run --coverage",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/vanilla/index.js",
	"type": "module",
	"types": "./dist/vanilla/index.d.ts"
}
